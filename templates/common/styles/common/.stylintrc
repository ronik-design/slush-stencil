{
  "alphabetical": false,
  "borderNone": true,
  "brackets": true,
  "colons": true,
  "colors": true,
  "commaSpace": true,
  "commentSpace": false,
  "cssLiteral": true,
  "depthLimit": false,
  "duplicates": true,
  "efficient": true,
  "emoji": false,
  "enforceBlockStyle": false,
  "enforceVarStyle": true,
  "extendPref": "@extend",
  "globalDupe": false,
  "indentSpaces": 2,
  "leadingZero": "always",
  "maxWarnings": 1,
  "maxWarningsKill": false,
  "mixed": true,
  "namingConvention": "BEM",
  "namingConventionStrict": true,
  "parenSpace": false,
  "placeholders": true,
  "quotePref": "single",
  "semicolons": true,
  "universal": false,
  "valid": true,
  "whitespace": true,
  "zeroUnits": true,
  "zIndexDuplicates": false,
  "zIndexNormalize": false
}
